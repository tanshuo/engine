define start:
    declare variable sp as vector2;
    declare function round 1;
    declare variable debug int;
    declare variable m_y int;
    <"position"> change to <0,0> in I;
enddef;

define update:

    sp set <1>;
    round sp;

enddef;

define round speed:
    get <"touch_position"> to speed;
    get <"position"> to debug;
    <"position"> change to speed in I;
    say debug;
enddef;
