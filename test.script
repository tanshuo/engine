define start:
    declare variable sp as vector2;
    declare function round 1;
    <"position"> change to <0,0> in I;
enddef;

define update:

    sp set <1>;
    round sp;

enddef;

define round speed:
    while speed smaller than <80>,
        calculate <"speed + 1">,
        speed set result,

    end;
enddef;
