define start:
    declare variable speed as vector3;
    declare variable t as float;
    declare variable delta as vector3;
    declare function round 1;
    t set <0,0>;
enddef;

define update:
    calculate <"t + 0.1">;
    say result;
    t set result;
    calculate <"sin(t) * 1">;
    speed set result;
    round speed;

enddef;

define round speed:
    move speed;
enddef;
